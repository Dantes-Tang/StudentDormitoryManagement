<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dantes.StudentDormitoryManagement.dao.AppFromDao">
    <sql id="table">appfrom</sql>
    <sql id = "items">FromId,StudentId,AppDate,AppType,AppDescribe,ApprStatus,
        Approver,ApprDate,ApprReason
    </sql>
    <select id="getAppFromList" resultMap="appFromMap">
        select * from <include refid="table"/> where 1=1
    </select>
    <resultMap id="appFromMap" type="com.dantes.StudentDormitoryManagement.model.AppFrom">
        <id column="FromId" property="fromId" jdbcType="INTEGER" />
        <result column="StudentId" property="studentId" jdbcType="INTEGER"/>
        <result column="AppDate" property="appDate" jdbcType="DATE"/>
        <result column="AppType" property="appType" jdbcType="INTEGER"/>
        <result column="AppDescribe" property="appDescribe" jdbcType="VARCHAR"/>
        <result column="ApprStatus" property="apprStatus" jdbcType="VARCHAR"/>
        <result column="Approver" property="apprOver" jdbcType="VARCHAR"/>
        <result column="ApprDate" property="apprDate" jdbcType="DATE"/>
        <result column="ApprReason" property="apprReason" jdbcType="VARCHAR"/>
    </resultMap>
</mapper>